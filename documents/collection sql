INSERT INTO `sandwiches` (`name`, `bread`, `grain`, `temperature`, `image`) VALUES ('blue cheese & salami', 2, 2, 2, 'bluechsalam.jpg');
INSERT INTO `sandwiches` (`name`, `bread`, `grain`, `temperature`, `image`) VALUES ('muffaleta', 4, 1, 1, 'muffaleta.jpg');
INSERT INTO `sandwiches` (`name`, `bread`, `grain`, `temperature`, `image`) VALUES ('mcbreakfast', 5, 1, 1, 'mcbreakfast.jpg');
INSERT INTO `sandwiches` (`name`, `bread`, `grain`, `temperature`, `image`) VALUES ('toasted ham & cheese', 6, 1, 1, 'toasthamch.jpg');
